    "test": "test"
  },
  "dependencies": {
    "dotenv": "^6.1.0",
    "truffle-hdwallet-provider": "^0.0.6",
    "bignumber.js": "^8.0.1",
    "react": "^16.6.1",
    "react-dom": "^16.6.1",
    "react-scripts": "2.1.1",
    "truffle-contract": "^4.0.0-next.0",
